    "test": "cross-env NODE_ENV=test nyc mocha ./server/tests/**/'*.test.js' --recursive --timeout 100000 --exit",
    "test:watch": "npm test --watch",
    "createModels": "babel-node ./server/helpers/dbScript/CreateTablesLocally.js",
    "createAnAdmin": "babel-node ./server/database/makeAdmin.js"
  },
